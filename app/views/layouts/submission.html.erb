<!DOCTYPE html>
<html>
<head>
  <link rel="icon" href="favicon.ico"/>

  <% if session[:submiss_title]  %>
    <title> Surface Science Spectra - WCF - <%= session[:submiss_title] %> </title>
  <% else  %>
    <title> Surface Science Spectra - WCF </title>
  <% end  %>

<!-- inserted 22 Mar 2012 -->
  <%= stylesheet_link_tag 'http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/themes/ui-lightness/jquery-ui.css' %>
  <%= stylesheet_link_tag 'active_scaffold.css' %>
  <%= stylesheet_link_tag "wecf", :media => "all" %>
  
  <%#= javascript_include_tag 'rails_jquery.js' %>
  <%= javascript_include_tag 'http://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.js' %>
  <%= javascript_include_tag 'http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/jquery-ui.js' %>
  <%= javascript_include_tag 'jquery-ui-timepicker-addon.js' %>
  <%= javascript_include_tag 'application.js' %>

  <%= csrf_meta_tag %>

  <%#= javascript_include_tag :defaults %>
  <%#= active_scaffold_includes %>

</head>
<body>

  <%= render(:partial => "layouts/banner") %>
  <%= render(:partial => 'layouts/site_nav_menu') %>
	<div id="div_spacer_1">  </div>
  <% if @show_nav_bar %>
    <% case session[:technique] %>
    <% when 'XPS/AES', nil %>
     <%= render(:partial => "layouts/submiss_nav_bar") %>
    <% when 'SIMS' %>
     <%= render(:partial => "layouts/sims_submiss_nav_bar") %>
    <% else %>
     <%= render(:partial => "layouts/submiss_nav_bar") %>
    <% end %>
  <% end %>

  <% if @show_nav_bar %>
    <div id="main" >
	<% else %>
    <div id="main_no_nav_bar" >
  <% end %>
		<% if flash[:notice]  %>
      <div id="notice"><%= flash[:notice] %> </div>
    <% end  %>
    <%#= @content_for_layout   ???JL %>
    <%= yield %>  
  </div>

</body>
</html>


