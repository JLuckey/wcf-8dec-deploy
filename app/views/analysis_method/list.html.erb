<h1>Listing analysis_methods</h1>

<table>
  <tr>
  <% for column in AnalysisMethod.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for analysis_method in @analysis_methods %>
  <tr>
  <% for column in AnalysisMethod.content_columns %>
    <td><%=h analysis_method.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => analysis_method %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => analysis_method %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => analysis_method }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @analysis_method_pages.current.previous } if @analysis_method_pages.current.previous %>
<%= link_to 'Next page', { :page => @analysis_method_pages.current.next } if @analysis_method_pages.current.next %> 

<br />

<%= link_to 'New analysis_method', :action => 'new' %>
